<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="ToolBar">
    <attr name="mode" format="integer" />
    <attr name="tool_bar_height" format="dimension" />
    <attr name="show_logo" format="boolean" />
    <attr name="show_title" format="boolean" />
    <attr name="title" format="string" />
    <attr name="title_text_color" format="color|reference" />
    <attr name="title_text_size" format="dimension" />
    <attr name="show_back" format="boolean" />
    <attr name="show_right_action_item" format="boolean" />
  </declare-styleable>

  <declare-styleable name="LoadLayout">
    <attr name="status">
      <enum name="loading" value="1" />
      <enum name="no_network" value="2" />
      <enum name="error" value="3" />
      <enum name="success" value="4" />
      <enum name="empty" value="5" />
    </attr>
  </declare-styleable>
</resources>